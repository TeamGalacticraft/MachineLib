{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "icon": "${mod_id}.png",
  "description": "${mod_description}",
  "license": "MIT",
  "contact": {
    "homepage": "https://galacticraft.dev/",
    "sources": "https://github.com/TeamGalacticraft/MachineLib/",
    "issues": "https://github.com/TeamGalacticraft/MachineLib/issues/"
  },
  "authors": [
    {
      "name": "Team Galacticraft",
      "contact": {
        "homepage": "https://github.com/TeamGalacticraft/",
        "discord": "https://discord.gg/n3QqhMYyFK"
      }
    }
  ],
  "contributors": [
    {
      "name": "marcus8448",
      "contact": {
        "homepage": "https://github.com/marcus8448"
      }
    }
  ],
  "environment": "*",
